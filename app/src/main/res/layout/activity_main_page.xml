<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#673AB7"
    android:backgroundTint="#C4D0D1"
    tools:context=".MainPage">


    <Button
        android:id="@+id/logoutbutton"
        android:layout_width="174dp"
        android:layout_height="55dp"
        android:layout_marginBottom="85dp"
        android:background="@color/purple_200"
        android:backgroundTint="@color/teal_200"
        android:text="LOGOUT"
        android:textColor="#311B92"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="113dp"
        android:layout_height="47dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="38dp"
        android:text="NAME"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/mainpagename"
        android:layout_width="206dp"
        android:layout_height="41dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="42dp"
        android:text="TextView"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/mainpageemail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.782"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="153dp"
        android:layout_height="48dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="58dp"
        android:text="EMAIL ADDRESS"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/mainpageemail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/mainpageemail"
        android:layout_width="197dp"
        android:layout_height="44dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="58dp"
        android:text="TextView"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/mainpageage"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="134dp"
        android:layout_height="39dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="57dp"
        android:text="AGE"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/logoutbutton"
        app:layout_constraintEnd_toStartOf="@+id/mainpageage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/mainpageage"
        android:layout_width="169dp"
        android:layout_height="41dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="55dp"
        android:text="TextView"
        android:textColor="#1C1B1B"
        android:textSize="17dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/logoutbutton"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="173dp"
        android:layout_height="180dp"
        android:layout_marginStart="218dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usericon" />
</androidx.constraintlayout.widget.ConstraintLayout>